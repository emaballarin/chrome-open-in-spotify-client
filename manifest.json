{
    "manifest_version": 2,
    "name": "Open in Spotify Desktop",
    "description": "Always open [open|play].spotify.com links in the Spotify Desktop app",
    "version": "1.4.1",
    "permissions": [
        "tabs",
        "webRequest",
        "webRequestBlocking",
        "*://open.spotify.com/",
        "*://play.spotify.com/"
    ],
    "background": {
        "page": "chrome-background.html"
    },
  "icons": {
  "128": "icon.png"
  }
}
